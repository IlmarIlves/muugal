{
	"name": "server",
	"version": "1.0.0",
	"main": "server.js",
	"license": "MIT",
	"scripts": {
		"start": "nodemon src/server.ts",
		"db:create-migration": "typeorm migration:create -n",
		"db:migrate": "ts-node ./scripts/migrate.ts",
		"db:seed": "ts-node ./scripts/getNewsData.ts"
	},
	"nodemonConfig": {
		"watch": [
			"src/**/*.ts"
		],
		"exec": "ts-node"
	},
	"dependencies": {
		"@types/express": "^4.17.2",
		"@types/node": "^13.13.52",
		"apollo-server-express": "^2.10.1",
		"express": "^4.17.1",
		"fast-glob": "^3.2.2",
		"graphql": "^14.6.0",
		"mysql": "^2.18.1",
		"nexus": "^0.12.0-rc.13",
		"node-fetch": "^2.6.0",
		"nodemon": "^2.0.2",
		"reflect-metadata": "^0.1.13",
		"ts-node": "^8.6.2",
		"typeorm": "^0.2.24",
		"typescript": "^3.8.2"
	}
}
